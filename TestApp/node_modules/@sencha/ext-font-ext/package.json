{
  "name": "@sencha/ext-font-ext",
  "version": "7.6.0",
  "sencha": {
    "name": "font-ext",
    "namespace": "Ext",
    "type": "code",
    "framework": "ext",
    "toolkit": "classic",
    "theme": "theme-neptune",
    "creator": "Sencha",
    "summary": "Extra font icons",
    "detailedDescription": "Simple web font with additional icons in the Font Awesome style",
    "version": "7.6.0",
    "compatVersion": "7.6.0",
    "format": "1",
    "output": "${framework.dir}/build/packages/${package.name}",
    "local": true,
    "resource": {
      "paths": [
        "${package.dir}/resources"
      ]
    },
    "resources": [
      {
        "path": "${package.dir}/resources",
        "output": "shared"
      }
    ],
    "sass": {
      "namespace": "Ext",
      "etc": "${package.dir}/sass/etc/all.scss",
      "var": "${package.dir}/sass/var",
      "src": [
        "${package.dir}/sass/src",
        "${package.dir}/sass/src/all.scss"
      ]
    },
    "classpath": "${package.dir}/src",
    "overrides": "${package.dir}/overrides",
    "slicer": null,
    "extTier": "pro"
  },
  "scripts": {
    "install": "node activate.js"
  },
  "license": "SEE LICENSE IN license.txt",
  "description": "Extra font icons",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/extjs/SDK.git"
  },
  "dependencies": {},
  "author": "Sencha, Inc.",
  "homepage": "https://www.sencha.com/products/extjs/",
  "bugs": "https://support.sencha.com/",
  "keywords": [
    "sencha",
    "extjs",
    "ext js",
    "ext-js",
    "ext"
  ]
}