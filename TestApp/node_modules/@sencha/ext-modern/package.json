{
  "name": "@sencha/ext-modern",
  "version": "7.6.0",
  "sencha": {
    "name": "modern",
    "namespace": "Ext",
    "type": "toolkit",
    "framework": "ext",
    "requires": [
      "core"
    ],
    "creator": "Sencha",
    "summary": "The Modern UI Toolkit",
    "detailedDescription": "UI components for modern browsers",
    "version": "7.6.0",
    "compatVersion": "7.6.0",
    "format": "1",
    "output": "${framework.dir}/build",
    "local": true,
    "sass": {
      "namespace": "Ext",
      "etc": "${package.dir}/sass/etc/all.scss",
      "var": "${package.dir}/sass/var",
      "src": "${package.dir}/sass/src"
    },
    "classpath": [
      "${package.dir}/src",
      "${package.dir}/../treegrid/src"
    ],
    "overrides": "${package.dir}/overrides",
    "buildOptions": {
      "modern": true
    },
    "language": {
      "js": {
        "input": {
          "version": "ES5"
        }
      }
    },
    "properties": {
      "skip.sass": 1,
      "skip.pkg": 1,
      "skip.slice": 1
    },
    "extTier": "pro"
  },
  "scripts": {
    "install": "node activate.js"
  },
  "license": "SEE LICENSE IN license.txt",
  "description": "The Modern UI Toolkit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/extjs/SDK.git"
  },
  "dependencies": {
    "@sencha/ext-modern-treegrid": "7.6.0"
  },
  "author": "Sencha, Inc.",
  "homepage": "https://www.sencha.com/products/extjs/",
  "bugs": "https://support.sencha.com/",
  "keywords": [
    "sencha",
    "extjs",
    "ext js",
    "ext-js",
    "ext"
  ]
}