{
  "name": "@sencha/ext-modern-theme-neptune",
  "version": "7.6.0",
  "sencha": {
    "name": "theme-neptune",
    "namespace": "Ext",
    "type": "theme",
    "extend": "theme-base",
    "framework": "ext",
    "toolkit": "modern",
    "requires": [
      "font-awesome"
    ],
    "creator": "Sencha",
    "summary": "Ext JS Modern Neptune Theme",
    "detailedDescription": "The Neptune theme provides a mostly flat/borderless look.",
    "version": "7.6.0",
    "compatVersion": "7.6.0",
    "format": "1",
    "output": "${workspace.subpkg.prefix}/${package.name}",
    "local": true,
    "sass": {
      "namespace": "Ext",
      "etc": "${package.dir}/sass/etc/all.scss",
      "var": "${package.dir}/sass/var",
      "src": "${package.dir}/sass/src"
    },
    "fashion": {
      "missingParameters": "error",
      "symbols": true
    },
    "classpath": "${package.dir}/src",
    "overrides": "${package.dir}/overrides",
    "properties": {
      "skip.pkg": 1
    },
    "extTier": "pro"
  },
  "scripts": {
    "install": "node activate.js"
  },
  "license": "SEE LICENSE IN license.txt",
  "description": "Ext JS Modern Neptune Theme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/extjs/SDK.git"
  },
  "dependencies": {
    "@sencha/ext-modern-theme-base": "7.6.0"
  },
  "author": "Sencha, Inc.",
  "homepage": "https://www.sencha.com/products/extjs/",
  "bugs": "https://support.sencha.com/",
  "keywords": [
    "sencha",
    "extjs",
    "ext js",
    "ext-js",
    "ext"
  ]
}